{% extends 'gestionsanteBundle::layout.html.twig' %}
{% block content %}


<div id="topOfPage" class="topTabsWrap">
    <div class="main">
        <div class="speedBar">
            <a class="home" href="{{  path('gestionsante_homepage')}}">Accueil</a>
            <span class="breadcrumbs_delimiter">
							<i class="icon-right-open-mini"></i>
						</span>
            <a class="all" href="{{  path('listePediatre')}}">Listes des pédiatres</a>
            <span class="breadcrumbs_delimiter">
							<i class="icon-right-open-mini"></i>
						</span>
            <span class="current">S'inscrire</span>
        </div>
        <h3 class="pageTitle h3">S'inscrire en tant que pédiatre</h3>
    </div>
</div>

<div class="mainWrap without_sidebar">
    <div class="main">
        <div class="content">
            <section class="post no_margin post_format_standard postAlter page type-page">
                <article class="post_content">
                    <div class="post_text_area">



                        <section class="no_padding">
                            <div class="container">
                                <div class="columnsWrap sc_columns sc_columns_count_3 margin_top_middle margin_bottom_middle">
                                    <div class="columns2_3 sc_column_item sc_column_item_1 odd first span_2">
                                        <div class="sc_contact_form sc_contact_form_contact contact_form_1">
                                            <h1 class="title">Coordonnées</h1>
                                            <form class="contact_1" method="post">
                                                <div class="columnsWrap">
                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_username">Nom</label>
                                                        <input type="text" name="nom" id="contact_form_username">
                                                    </div>
                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_username">Prenom</label>
                                                        <input type="text" name="prenom" id="contact_form_username">
                                                    </div>
                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_email">E-mail</label>
                                                        <input type="text" name="email" id="contact_form_email">
                                                    </div>
                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_email">Numero de telephone</label>
                                                        <input type="text" name="num" id="contact_form_email">
                                                    </div>
                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_subj">Adresse</label>
                                                        <input type="text" name="adresse" id="contact_form_subj">
                                                    </div>

                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_username">Prix de consultation</label>
                                                        <input type="text" name="prix" id="contact_form_username">
                                                    </div>

                                                    <div class="columns1_3">
                                                        <label class="required" for="contact_form_username">Spécialité</label>
                                                        <input type="text" name="specialite" id="contact_form_username">
                                                    </div>
                                                </div>

                                                <div class="message">
                                                    <div class="">
                                                        <label class="required" for="contact_form_message">Description</label>
                                                        <textarea id="contact_form_message" class="textAreaSize inited" name="description" style="overflow: hidden; word-wrap: break-word; resize: none; height: 64px;"></textarea>
                                                    </div>
                                                </div>
                                                <div class="message">
                                                    <div class="">
                                                        <label class="required" for="contact_form_message">Formation</label>
                                                        <textarea id="contact_form_message" class="textAreaSize inited" name="formation" style="overflow: hidden; word-wrap: break-word; resize: none; height: 64px;"></textarea>
                                                    </div>
                                                </div>

                                                <div class="message">
                                                    <div class="">
                                                        <label class="required" for="contact_form_message">Parcours professionnel</label>
                                                        <textarea id="contact_form_message" class="textAreaSize inited" name="parcours" style="overflow: hidden; word-wrap: break-word; resize: none; height: 64px;"></textarea>

                                                    </div>
                                                </div>
                                                <div class="columns1_3 sc_column_item sc_column_item_3 odd after_span_2">
                                                        <label class="required" for="contact_form_message">Photo</label>
                                                        <input type="file" name="image" class="button">
                                                </div>

                                                <div class="sc_contact_form_button">
                                                    <div class="squareButton sc_button_size sc_button_style_global global ico">
                                                        <button type="submit" name="contact_submit" class="contact_form_submit icon-comment-1">envoyer demande</button>
                                                    </div>
                                                </div>

                                            </form>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </section>

                    </div>
                </article>
            </section>
        </div>
    </div>
</div>

{% endblock %}

